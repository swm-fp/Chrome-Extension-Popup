{
    "manifest_version": 2,
  
    "name": "FLOWER",
    "description": "get bookmarks",
    "version": "1.0",
  
    "browser_action": {
      "default_icon": "icon.png"
    },

    "background":{
        "scripts":["background.js"],
        "persistent": true
    },

    "permissions": [
      "activeTab",
      "storage",
      "<all_urls>",
      "bookmarks"
    ]
  }
  